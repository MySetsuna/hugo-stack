&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="nb">Array&lt;/span>&lt;span class="p">.&lt;/span>&lt;span class="nx">prototype&lt;/span>&lt;span class="p">.&lt;/span>&lt;span class="nx">slice&lt;/span>&lt;span class="p">(&lt;/span>&lt;span class="nx">startIndex?&lt;/span>:&lt;span class="kt">number&lt;/span>&lt;span class="p">,&lt;/span> &lt;span class="nx">endIndex?&lt;/span>:&lt;span class="kt">number&lt;/span>&lt;span class="p">)&lt;/span>
